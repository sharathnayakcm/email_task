    <tr>
      <td rowspan="4">Left Pane Icons</td>
      <td><span class="settings"></span></td>
      <td>Settings</td>
      <td>Edit CUG</td>
    </tr>

    <tr>
      <td><span class="on_star"></span></td>
      <td>Make Favorite</td>
