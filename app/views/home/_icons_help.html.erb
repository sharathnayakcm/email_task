<div class="content icons_explanations">
  <table class="dozz_grid channel channel_icons">
    <tr>
      <th>Group</th>
      <th>Icons</th>
      <th>Name</th>
      <th>Explanation</th>
    </tr>

    <tr>
      <td rowspan="4">Top Pane Icons</td>
      <td><span class="home"></span></td>
      <td>Home</td>
      <td>My Home Page</td>
    </tr>

    <tr>
      <td><span class="buzz_icon"></span></td>
      <td>New Buzz</td>
      <td>Write a New Buzz</td>
    </tr>

    <tr>
      <td><span class="profile"></span></td>
      <td>Profile</td>
      <td>My Profile Page</td>
    </tr>

    <tr>
      <td><span class="logout"></span></td>
      <td>Logout</td>
      <td>Logout</td>
    </tr>


    <tr>
      <td rowspan="4">Left Pane Icons</td>
      <td><span class="settings"></span></td>
      <td>Settings</td>
      <td>Edit Channel</td>
    </tr>

    <tr>
      <td><span class="on_star"></span></td>
      <td>Make Favorite</td>
      <td>Favorite Channel</td>
    </tr>

    <tr>
      <td><span class="off_star"></span></td>
      <td>Make Unfavorite</td>
      <td>Favorite Channel</td>
    </tr>

    <tr>
      <td><span class="add_channel"></span></td>
      <td>Add Channel</td>
      <td>Add a New Channel</td>
    </tr>


    <tr>
      <td rowspan="2">Buzzout/Rezzout Icons</td>
      <td><span><%= image_tag("attachment_big.png") %></span></td>
      <td>Attachment</td>
      <td></td>
    </tr>

    <tr>
      <td><span><%= image_tag("buzzout.png") %></span></td>
      <td>Buzzout</td>
      <td>Buzzout</td>
    </tr>

    <tr>
      <td rowspan="2" id="row_span_cug">Buzz Icons</td>
      <td><span class="rezz"></span></td>
      <td>Rezzout</td>
      <td>View/Create Rezz</td>
    </tr>


    <tr>
      <td><span class="delete"></span></td>
      <td>Delete Buzz</td>
      <td>Remove Buzz</td>
    </tr>
  </table>


  <table class="dozz_grid channel cug_icons">
    <tr>
      <th>Group</th>
      <th>Icons</th>
      <th>Name</th>
      <th>Explanation</th>
    </tr>

    <tr>
      <td rowspan="4">Top Pane Icons</td>
