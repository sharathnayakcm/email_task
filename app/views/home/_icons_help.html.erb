    <tr>
      <td><span><%= image_tag("buzzout.png") %></span></td>
      <td>Buzzout</td>
      <td>Buzzout</td>
    </tr>



    <tr>
      <td rowspan="7" id="row_span_cug">Buzz Icons</td>
      <td><span class="rezz"></span></td>
